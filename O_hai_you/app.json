{
  "expo": {
    "name": "O_Hai_You",
    "slug": "O_Hai_You",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash_animation.gif",
      "resizeMode": "contain",
      "backgroundColor": "#ff85ac"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ff85ac"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "A Japanese language-focused mobile application where users can learn and practice Japanese vocabulary, grammar, and pronunciation.",
    "githubUrl": "https://github.com/PinoyColada/O-hai-you-front-end"
  }
}
